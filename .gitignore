      var check_2 = "1";
      var check_3 = "0";

      $(function(){

        $("#consent2").on("click",function(){
          if($(this).is(":checked")==true)
          {
            check_2 = "1";
          }
          else
          {
            check_2 = "0";
          }
        });

        $("#consent3").on("click",function(){
          if($(this).is(":checked")==true)
          {
            check_3 = "1";
          }
          else
          {
            check_3 = "0";
          }
        });

        $("#accept_all").on("click",function(){
          $("#consent2,#consent3").prop('checked',true);
          check_2 = "1";
          check_3 = "1";

          //$(".C_BTN_Cookie_Policy_Accept").trigger("click");
          setCookie();
        });

        $(".C_BTN_Cookie_Policy").on("click",function(){
          //$(".C_BTN_Cookie_Policy_Accept").trigger("click");
          setCookie();
        });

        $(".C_BTN_Cookie_Policy_Accept").click(function () {

            check_2 = "1";
            check_3 = "1";
            setCookie();
            
        });

        $("#footer_cookie_policy").on("click",function(){
          $(".view--modal").trigger('click');
        });
      });

      function setCookie()
      {
        $.ajax({
            url: '/SetCookie.aspx',
            type: 'POST',
            data: {
                DataKey: "SetCookie", url: window.location.href,"consent2":check_2,"consent3":check_3
            },
            success: function (data) {
                if (parseInt(data) == 1) {
                    $(".C_Cookie_Policy").fadeOut();
                    location.reload();
                }
            },
            error: function(){
            }
        });
      }


